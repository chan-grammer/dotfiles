alias rm="rm -i"
alias naunodesk="nautilus --no-desktop"
alias tohtdocs="cd /opt/lampp/htdocs"
alias homeshick="source $HOME/.homesick/repos/homeshick/bin/homeshick.sh"
alias phpini="sudo vim /opt/lampp/etc/php.ini"
alias apacheconf=" sudo vim /opt/lampp/etc/httpd.conf"
alias apachevhosts="sudo vim /opt/lampp/etc/extra/httpd-vhosts.conf"
alias mysqlcnf="sudo vim /opt/lampp/etc/my.cnf"
alias xamppconfig="sudo /opt/lampp/xampp"
alias phpserver="php -S localhost:8080"
alias edithosts="sudo vim /etc/hosts"
alias editalias="vim ~/.aliases"
